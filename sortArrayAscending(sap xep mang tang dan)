public static void resever(int[] arr) {
		 for (int i = 0; i < arr.length; i++) {
	            for (int j = i + 1; j < arr.length; j++) {
	                if (arr[i] > arr[j]) {
	                    int check = arr[i];
	                    arr[i] = arr[j];
	                    arr[j] = check;
	                }
	            }
	        }
